# Wap to find the prime number

def prime():
    count = 0
    if num == 2:
        print("Prime Number")
    else:
        for i in range(2, num):
            if num % i != 0:
                print("Prime Number")
                break
            else:
                count += 1
                break
        if count == 1:
            print("Not Prime Number")
num = int(input("Enter a number : "))
prime()
